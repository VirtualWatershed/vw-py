{
    "orig_epsg": "$orig_epsg",
    "description": "$description",
    "taxonomy": "$tax",
    "basename": "$basename",
    "parent_model_run_uuid": "$parent_model_run_uuid",
    "model_run_uuid": "$model_run_uuid",
    "model_vars": "$model_vars",
    "model_set": "$model_set",
    "model_set_type": "$model_set_type",
    "model_set_taxonomy": "$model_set_taxonomy",
    "spatial": {
        "geomtype": "grid",
        "records":"$recs",
        "epsg": "$epsg",
        "features":"$features",
        "bbox": "$westBnd,$southBnd,$eastBnd,$northBnd"
    },
    "dates": {
        "start": "$start_datetime",
        "end": "$end_datetime"
    },
    "records": "$recs",
    "active": "true",
    "services": [
    "$wms",
    "$wcs"
    ],
    "categories": [{
            "location": "$location",
            "modelname": "$modelname",
            "state": "$state"
        }],
    "sources": [{
        "mimetype": "$mimetype",
        "files": ["$inputFilePath"],
        "set": "original",
        "external": "False",
        "extension": "$ext"
    }],
    "standards": [
        "FGDC-STD-001-1998",
        "ISO-19115:2003",
        "ISO-19119",
        "ISO-19110"
    ],
    "formats": [
        "$ext"
    ],
    "metadata": {
        "xml": "$fgdcMetadata",
        "upgrade": "true",
        "standard": "FGDC-STD-001-1998"
    },
    "apps": ["my_app"]
}
