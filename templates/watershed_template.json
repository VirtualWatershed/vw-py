{
    "orig_epsg": "{% if orig_epsg %}{{ orig_epsg }}{% else %}99999{% endif %}",
    "description": "{{ description }}",
    "taxonomy": "file",
    "basename": "{{ basename }}",
    "parent_model_run_uuid": "{{ parent_model_run_uuid }}",
    "model_run_uuid": "{{ model_run_uuid }}",
    "model_vars": "{% if model_vars %}{{ model_vars }}{% else %}unknown{% endif %}",
    "model_set": "{{ model_set }}",
    "model_set_type": "{{ model_set_type }}",
    "model_set_taxonomy": "{{ model_set_taxonomy }}",
    "spatial": {
        "geomtype": "{{ model_set_taxonomy }}",
        "records":"1",
        "epsg": "{% if epsg %}{{ epsg }}{% else %}99999{% endif %}",
        "features":"1",
        "bbox": "{{ west_bound }},{{ south_bound }},{{ east_bound }},{{ north_bound }}"
    },
    "dates": {
        "start": "{{ start_datetime }}",
        "end": "{{ end_datetime }}"
    },
    "records": "1",
    "active": "true",
    "services": [
    "{{ wms_str }}",
    "{{ wcs_str }}"
    ],
    "categories": [{
            "location": "{{ watershed_name }}",
            "modelname": "{% if model_name %}{{ model_name }}{% else %}reference{% endif %}",
            "state": "{{ state }}"
        }],
    "sources": [{
        "mimetype": "{{ mimetype }}",
        "files": ["{{ input_file_path }}"],
        "set": "original",
        "external": "False",
        "extension": "{{ ext }}"
    }],
    "standards": [
        "FGDC-STD-001-1998",
        "ISO-19115:2003",
        "ISO-19119",
        "ISO-19110"
    ],
    "formats": [
        "{{ ext }}"
    ],
    "metadata": {
        "xml": "{{ fgdc_metadata }}",
        "upgrade": "true",
        "standard": "FGDC-STD-001-1998"
    },
    "apps": ["vwp"]
}
